    <!-- Scripts -->
    
    <script>
        // Sample agent data with realistic configurations
        const sampleAgents = [
            {
                id: 'agent-006',
                name: 'WebScraper Intelligence',
                type: 'autonomous',
                status: 'active',
                model: 'gpt-4',
